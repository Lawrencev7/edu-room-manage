<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.room.manage.mapper.ApprovalMapper">
    <select id="selectDto" resultType="edu.room.manage.dto.ApprovalDTO">
        select a.*,u.name username,u.serial_Number serialNumber from approval a
        left join user u on u.id = a.user_id
        <include refid="BaseMapper.Example_Where_Clause"/>
        <include refid="BaseMapper.Order_Sql_Clause"/>
    </select>
</mapper>