<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.room.manage.mapper.UserMapper">


    <!--删除用户和用户关联表-->
    <delete id="deleteById">
        delete
        from user
        where id = #{id};
    </delete>
    <select id="selectByUsername" resultType="edu.room.manage.domain.User">
        select *
        from user
        where username = #{uername};
    </select>

</mapper>